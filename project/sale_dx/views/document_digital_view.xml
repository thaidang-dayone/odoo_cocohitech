<odoo>
    <record id="view_document_digital_tree" model="ir.ui.view">
        <field name="name">document.digital.tree</field>
        <field name="model">document.digital</field>
        <field name="arch" type="xml">
            <tree string="Document Digital" editable="bottom">
                <field name="name"/>
                <field name="currency_id" />
                <field name="description"/>
                <field name="active"/>
            </tree>
        </field>
    </record>

    <record id="action_document_digital" model="ir.actions.act_window">
        <field name="name">Document Digital</field>
        <field name="res_model">document.digital</field>
        <field name="view_mode">tree</field>
    </record>

    <menuitem id="menu_document_digital" 
        name="Document Type" 
        parent="sale.menu_sale_config" 
        action="action_document_digital"/>
</odoo>